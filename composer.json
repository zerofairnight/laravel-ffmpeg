{
    "name": "zerofairnight/laravel-ffmpeg",
    "description": "Laravel FFMpeg integraion",
    "keywords": ["ffmpeg", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "zerofairnight",
            "email": "zerofairnight@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5",
        "symfony/process": "^4.0",
        "php-ffmpeg/php-ffmpeg": "^0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\FFMpeg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\FFMpeg\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\FFMpeg\\FFMpegServiceProvider"
            ],
            "aliases": {
                "FFMpeg": "Laravel\\FFMpeg\\Facades\\FFMpeg",
                "FFProbe": "Laravel\\FFMpeg\\Facades\\FFProbe"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
